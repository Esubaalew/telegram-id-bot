{
  "version": 2,
  "builds": [
    {
      "src": "api/webhook/Cargo.toml",
      "use": "vercel-rust"
    },
    {
      "src": "api/health/Cargo.toml", 
      "use": "vercel-rust"
    }
  ],
  "routes": [
    {
      "src": "/webhook",
      "dest": "/api/webhook"
    },
    {
      "src": "/health",
      "dest": "/api/health"
    },
    {
      "src": "/",
      "dest": "/api/health"
    }
  ]
}